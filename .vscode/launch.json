    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "CGO_ENABLED": "0"
            },
            "args": []
        },
        {
            "name": "Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "CGO_ENABLED": "0"
            },
            "args": [
                "run",
                "--verbose=true",
                "--debug-webui-proxy=http://localhost:3000",
                "testdata/example-config.json"
            ]
        }
    ]
}